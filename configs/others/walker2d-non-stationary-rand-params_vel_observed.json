{
    "env_name": "walker2d-non-stationary-rand-params",
    "env_params": {
        "n_train_tasks": 100,
        "n_eval_tasks": 30,
        "use_normalized_env": true,
        "filename": "walker2d.xml",
        "frame_skip": 4,
        "rand_params": ["body_mass", "dof_damping", "body_inertia", "geom_friction"],
        "log_scale_limit": 1,
        "observe_velocity": true,
        "change_mode": "location",
        "termination_possible": true,
        "positive_change_point_basis": 7,
        "negative_change_point_basis": -7,
        "change_point_interval": 2,
        "state_reconstruction_clip": 8
    },
    "algo_params": {
        "use_relabeler": true,
        "use_combination_trainer": false,
        "use_data_normalization": true,
        "use_parametrized_alpha": false,
        "encoding_mode": "transitionSharedY",
        "use_fixed_seeding": true,
        "batch_size_reconstruction": 256,
        "batch_size_combination": 256,
        "batch_size_policy": 256,
        "batch_size_relabel": 1024,
        "time_steps": 10,
        "latent_size": 6,
        "sac_layer_size": 300,
        "max_replay_buffer_size": 100000000,
        "data_usage_reconstruction": null,
        "data_usage_sac": null,
        "num_last_samples": 100000000,
        "permute_samples": false,
        "num_train_epochs": 501,
        "snapshot_gap": 10,
        "num_reconstruction_steps": 5000,
        "num_policy_steps": 2000,
        "num_train_tasks_per_episode": 25,
        "num_transitions_initial": 800,
        "num_transitions_per_episode": 800,
        "num_eval_trajectories": 2,
        "showcase_every": 0,
        "num_showcase_deterministic": 1,
        "num_showcase_non_deterministic": 1,
        "max_path_length": 800,
        "target_entropy_factor": 1.0,
        "sac_alpha": 1.0
    },
    "reconstruction_params": {
        "use_state_diff": false,
        "component_constraint_learning": false,
        "prior_mode": "fixedOnY",
        "prior_sigma": 0.5,
        "num_classes": 1,
        "lr_encoder": 0.0003,
        "lr_decoder": 0.0003,
        "alpha_kl_z": 0.001,
        "beta_kl_y": 0.001,
        "net_complex_enc_dec": 10.0,
        "factor_qf_loss": 1.0,
        "train_val_percent": 0.8,
        "eval_interval": 50,
        "early_stopping_threshold": 500,
        "temp_folder": ".temp_cemrl"
    }
}
